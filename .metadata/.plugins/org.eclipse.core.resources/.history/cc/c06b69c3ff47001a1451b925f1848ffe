package test;

import java.util.Scanner;

public class Program {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a no ; ");
		int num = sc.nextInt();
		
		int temp = num;
		int sum = 0, rem, digits=0;
		
		while(temp!=0)
		{
			temp = temp/10;
			digits++;
		}
	    System.out.println(digits);
		
	}

}
