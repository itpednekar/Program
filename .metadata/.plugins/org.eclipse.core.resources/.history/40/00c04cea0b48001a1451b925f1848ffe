package test;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a no : ");
		int num = sc.nextInt();
		
		int n = num;
		boolean flag = true;
		
		for(int i = 2; i<n/2; i++)
		{
			if(num%i == 0)
			{
				flag = false;
				break;
			}
		}
		
		if(flag)
			System.out.println("Prime no");
		else
			System.out.println("NOT prime No");
		

	}

}
