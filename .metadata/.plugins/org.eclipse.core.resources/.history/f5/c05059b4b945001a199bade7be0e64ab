
public class Program 
{
	
	public static void main(String[] args) {
		boolean k;
		for (int i = 1; i <= 5; i++) 
		{
			k=true;
			int n = 1;
			for (int j = 1; j<=9; j++) {
				if(j >= 6-i && j  <= 4+i && k)
				{
					if(n<=j)
					{
				      System.out.print(i);
				      n++;
					}
					else
						n--;
				    k=false;
				    
				}
				else
				{
					System.out.print(" ");
					k=true;
				}
			}
			System.out.println();
		}
	}

}
